-script
fn:amc
"$PWD"
--output
$HOME/plex
--log-file
$HOME/logs/log.file
--action
move
--conflict
auto
-non-strict
--def
seriesFormat=@$HOME/orionsbelt/filebot/schemes/tv.groovy
animeFormat=@$HOME/orionsbelt/filebot/schemes/anime.groovy
movieFormat=@$HOME/orionsbelt/filebot/schemes/movie.groovy
musicFormat=@$HOME/orionsbelt/filebot/schemes/music.groovy
minFileSize=0
minLengthMS=0
excludeList=amc.excludes